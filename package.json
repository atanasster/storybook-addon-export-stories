  },
  "license": "MIT",
  "author": "Atanas Stoyanov",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "storybook": "start-storybook -p 6001",
    "build": "rollup -c && tsc",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "pub": "pushd ./ && npm run build && cd dist && npm publish && popd"
  },
