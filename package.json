{
  "name": "storybook-addon-export-stories",
  "version": "1.0.1",
  "description": "A storybook addon to export all stories to a json file",
  "keywords": [
    "addon",
    "react",
    "storybook"
  ],
  "homepage": "https://github.com/atanasster/storybook-addon-export-stories/",
